<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension point="org.eclipse.ui.newWizards">
      <wizard
            id="edu.isistan.ucseditor.wizards.UCSWizard"
            name="%UCSWizard_Label"
            class="edu.isistan.ucseditor.wizards.UCSWizard"
            category="edu.isistan.reassistant.WizardCategory"
            icon="icons/usecase.gif">
         <description>%UCSWizard_Description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="edu.isistan.ucseditor.editor.UCSEditor"
            id="edu.isistan.ucseditor.editor.UCSEditor"
            name="%UCSEditor_Name"
            extensions="%UCSEditor_FilenameExtension"
            icon="icons/usecase.gif"
            contributorClass="edu.isistan.ucseditor.editor.UCSEditorContributor">
      </editor>
   </extension>
</plugin>
